{ 

	"version": "1.4", 
	"manifest_version": 2, 
	"name": "WhatsLog",
	"description": "Whatsapp status logger by ThirtySix.", 	
	
	"browser_action": {
		"default_title": "WhatsLog",
		"default_popup": "popup.html",
		"default_icon": {
			"48": "36.png"
		}
	},
	
	"icons": {
		"48": "36.png"
	},
	
	"content_scripts": [ { 
		"matches": ["*://*.whatsapp.com/*"], 
		"js": ["whatslog.js"], 
		"all_frames": true		
	} ]
	
}